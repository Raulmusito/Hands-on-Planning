<launch>
    <param name="~frame_id" type="string" value="odom" />
    <include file="$(find path_planning_ompl)/launch/turtlebot3_home_sim.launch" />
    <node pkg="path_planning_ompl" type="laser_scan_to_point_cloud.py" name="laserscan_to_pointcloud"/>
    <node pkg="octomap_server" type="octomap_server_node" name="octomap" output="screen">
        <remap to="/laserscan_to_pointcloud/converted_pc" from="/cloud_in"/>
    </node>
    <include file="$(find turtlebot3_teleop)/launch/turtlebot3_teleop_key.launch" /> 
</launch>
